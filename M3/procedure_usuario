create or replace procedure userCreate
is
existe int;

begin

select count(*) into existe from users;

if existe = 0 then
    INSERT INTO USERS
    Values(1,'Pedri', '26/09/2003','P@ssw0rd');        
else
    delete from users where 1=ID_USER;
    INSERT INTO USERS
    Values(1,'Pedri', '26/09/2003','P@ssw0rd');     
end if;

end;
