create or replace procedure userCreate(userID INT, usernameUser VARCHAR, fecha DATE, passwordUser VARCHAR)
is
existe int;

begin

select count(*) into existe from users;

if existe = 0 then
    INSERT INTO USERS
    Values(userID,usernameUser, fecha,passwordUser);        
else
    delete from users where userID=ID_USER;
    INSERT INTO USERS
    Values(userID,usernameUser, fecha,passwordUser);    
end if;

end;
